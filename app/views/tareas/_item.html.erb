<%
=begin%>
 <div name ="tarea_<%= item.id%>" style="background-color: #e9e9e9">
	<h4><%= item.nombre %></h4>
	<p>revisor: <%= item.revisor.nombre%></p>
	<p>integrante: <%= item.integrante.nombre%></p>

	<p>estado <%= item.estado%></p>
	

	<% if @idUsuario == item.integrante_id %>
	
	<input type="button" value="Terminar"
	/>
	
	<% end %>
</div> 
<%
=end%>

<tr>
	<td><%= item.nombre %></td>
	<td><%= item.revisor.nombre %> <%= item.revisor.apellido %></td>
	<td><%= item.integrante.nombre %> <%= item.integrante.apellido %></td>
	<td><%= item.estado %></td>
	<% if @idUsuario == item.integrante_id %>
		<td>
			<input type="button" value="Terminar" />
		</td>
	<% end %>
</tr>