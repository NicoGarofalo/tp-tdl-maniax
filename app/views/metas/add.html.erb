<%= form_for meta, url: metas_path, method: :post do |f| %>
<div class="container">
  <%= hidden_field_tag "meta[proyecto_id]", idProyecto %>

  <div class="form-group">
    <%= f.label :nombre %>
    <%= f.text_field :nombre, class: "form-control" %>
  </div>
  <p> asdsa<%=idProyecto%></p>

  <div class="form-group">
    <%= f.label :descripcion %>
    <%= f.text_area :descripcion, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :fecha_vencimiento %>
    <%= f.date_field :fecha_vencimiento, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit "Agregar Meta", class: "btn btn-primary" %>
  </div>
</div>

<% end %>